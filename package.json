{
  "name": "@phoenixillusion/babylonjs-trenchbroom-loader",
  "private": true,
  "version": "1.0.3",
  "type": "module",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && cp -r src/hxlibmap dist/."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PhoenixIllusion/babylonjs-trenchbroom-loader"
  },
  "devDependencies": {
    "typescript": "^5.0.2"
  },
  "keywords": ["trenchbroom", "babylonjs"],
  "description": "A library for loading of Trenchbroom MAP level files.",
  "author": "Phoenix Illusion",
  "dependencies": {
    "@babylonjs/core": "6.25.0",
    "@babylonjs/loaders": "6.27.0"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    } ,
    "./map-loader": {
      "default": "./dist/map-loader.js",
      "types": "./dist/map-loader.d.ts"
    },
    "./mesh-builder": {
      "default": "./dist/mesh-builder.js",
      "types": "./dist/mesh-builder.d.ts"
    }, 
    "./hxlibmap": {
      "default": "./dist/hxlibmap/index.js",
      "types": "./dist/hxlibmap/index.d.ts"
    } 
  },
  "files": [
    "README.md",
    "haxe/hxlibmap-LICENSE",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/map-loader.d.ts",
    "dist/map-loader.js",
    "dist/mesh-builder.d.ts",
    "dist/mesh-builder.js",
    "dist/hxlibmap/index.d.ts",
    "dist/hxlibmap/index.js"
  ]
}
